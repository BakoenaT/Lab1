.data
prompt: .asciz "Enter number of cattle: "
not_enough: .asciz "Not enough for bohali\n"
small_herd: .asciz "Small herd\n"
wealthy: .asciz "Weathly cattle owener\n"

.text
.globl main
main:
#Prompt user
la a0, prompt
li a7, 4
ecall
#Read cattle count
li a7, 5
ecall
mv t0, a0 # t0 = cattle
# If cattle < 8 = Not enough for bohali
li t1, 8
blt t0, t1, not_bohali
# If cattle <= 20 = Small herd
li t1, 20
ble t0, t1, small
#Else = Wealthy cattle owner
la a0, wealthy
li a7, 4
ecall
j exit
not_bohali:
la a0, not_enough
li a7, 4
ecall
j exit
small:
la a0, small_herd
li a7, 4
ecall
j exit
exit:
li a7, 10
ecall
